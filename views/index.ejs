<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Salesforce Contacts</title>
    <style>
        body {
            background-color: black;
            color: green;
            font-family: monospace;
            padding: 20px;
        }
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid green;
            padding: 10px;
            text-align: left;
        }
    </style>
</head>
<body>
    <h1>Salesforce Contacts</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Added Date</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            <% contacts.forEach(contact => { %>
                <tr>
                    <td><%= contact.name %></td>
                    <td><%= new Date(contact.addedDate).toLocaleString() %></td>
                    <td><%= contact.status %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
